{
  "name": "code-coevoer",
  "displayName": "code-coevoer",
  "description": "本插件实现了对git commit进行实时检测，提醒并帮助用户进行过时测试代码与生产代码之间的实时更新。",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:.git"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "Code Coevoer",
      "properties": {
        "code-coevoer.enable": {
          "type": "boolean",
          "default": true,
          "description": "Enable/Disable Code Coevoer"
        },
        "code-coevoer.apikey": {
          "type": "string",
          "default": "",
          "description": "API key for LLM"
        },
        "code-coevoer.baseURL": {
          "type": "string",
          "default": "https://api.openai.com/v1",
          "description": "Base URL for LLM API"
        },
        "code-coevoer.language": {
          "type": "string",
          "enum": [
            "Java",
            "Python",
            "JavaScript",
            "C++",
            "C#",
            "Go",
            "Ruby",
            "Swift"
          ],
          "default": "Go",
          "description": "Select the programming language for your project"
        }
      }
    },
    "commands": [
      {
        "command": "code-coevoer.start",
        "title": "begin check"
      },
      {
        "command": "code-coevoer.showSidebar",
        "title": "Show Sidebar"
      },
      {
        "command": "code-coevoer.showQuickPick",
        "title": "打开 Code-Coevoer 设置"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "code-coevoer-sidebar",
          "title": "Code-Coevoer",
          "icon": "resources/poo.svg"
        }
      ]
    },
    "views": {
      "code-coevoer-sidebar": [
        {
          "type": "webview",
          "id": "code-coevoer.chatView",
          "name": "可能未更新的测试代码",
          "visibility": "visible"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.24.0",
    "eslint": "^9.13.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "axios": "^1.7.8",
    "chokidar": "^4.0.1",
    "dotenv": "^16.4.5",
    "openai": "^4.73.1",
    "simple-git": "^3.27.0"
  }
}